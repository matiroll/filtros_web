<h1>New filtro_vehiculo</h1>

<% form_for(@filtro_vehiculo) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :id %><br />
    <%= f.text_field :id %>
  </p>
  <p>
    <%= f.label :vehiculo %><br />
    <%= f.text_field :vehiculo %>
  </p>
  <p>
    <%= f.label :filtro %><br />
    <%= f.text_field :filtro %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', filtro_vehiculos_path %>