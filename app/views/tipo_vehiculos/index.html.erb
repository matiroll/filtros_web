<h1>Listing tipo_vehiculos</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Nombre</th>
  </tr>

<% @tipo_vehiculos.each do |tipo_vehiculo| %>
  <tr>
    <td><%=h tipo_vehiculo.id %></td>
    <td><%=h tipo_vehiculo.nombre %></td>
    <td><%= link_to 'Show', tipo_vehiculo %></td>
    <td><%= link_to 'Edit', edit_tipo_vehiculo_path(tipo_vehiculo) %></td>
    <td><%= link_to 'Destroy', tipo_vehiculo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New tipo_vehiculo', new_tipo_vehiculo_path %>